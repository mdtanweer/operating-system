	for(i=0;i<n;i++)
 	{
 	   avgwt=avgwt+wt[i];
 	   avgtat=avgtat+tat[i];
    }
 	printf("process \t At\t Bt\t Ct\t Tat\t Wt\n");
 	for(i=0;i<n;i++)
 	{
 		printf("P%d\t\t %d\t %d\t %d\t %d\t %d\n",(i+1),at[i],temp[i],ct[i],tat[i],wt[i]);
	}
	printf("Average waiting time =%f\n",avgwt/n);
	printf("Average turnaround time=%f\n",avgtat/n);
     return 0;     
}
